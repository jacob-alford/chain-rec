{
  "plugins": [
    ["annotate-pure-calls"],
    [
      "transform-rename-import",
      {
        "replacements": [
          { "original": "^fp-ts/(\\w*)$", "replacement": "fp-ts/es6/$1.js" },
          {
            "original": "^fp-ts-contrib/(\\w*)$",
            "replacement": "fp-ts-contrib/es6/$1.js"
          }
        ]
      }
    ]
  ]
}
